--Load
local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/hakaiofc/Base/main/HUBORION')))()
--Main Script
local Window = OrionLib:MakeWindow({Name = "THE BRAZILIAN'S KAI DOJOü•ã HUB EXCLUSIVO", HidePremium = false, SaveConfig = true, ConfigFolder = "OrionTest", IntroEnabled = false})
  
--ANTI AFK 
pcall(function()
    local VirtualUser=game:service'VirtualUser'
    game:service'Players'.LocalPlayer.Idled:connect(function()
    warn("anti-afk")
    VirtualUser:CaptureController()
    VirtualUser:ClickButton2(Vector2.new())
       end)
    end) 

--GLOBALS 
LP = game.Players.LocalPlayer
VIM = game:GetService("VirtualInputManager")

--Valor
_G.AutoDPB = True
--Fun√ß√£o
function AutoDPB()
while _G.AutoDPB == true do
local Event = game:GetService("ReplicatedStorage").Remotes.Dumbbell
Event:FireServer()
local Event = game:GetService("ReplicatedStorage").Remotes.Pushup
Event:FireServer()
local Event = game:GetService("ReplicatedStorage").Remotes.BenchToggle
Event:FireServer()
wait(0.485)
end
end
--Valor
_G.AutoGolpe = True
--Fun√ß√£o
function AutoGolpe()
while _G.AutoGolpe == true do
local A_1 = "Left Punch"
local Event = game:GetService("ReplicatedStorage").Remotes.Attack
Event:FireServer(A_1)
local A_2 = "Right Punch"
local Event = game:GetService("ReplicatedStorage").Remotes.Attack
Event:FireServer(A_2)
wait()
end
end
--Valor
_G.AutoDP = True
--Fun√ß√£o
function AutoDP()
while _G.AutoDP == true do
local Event = game:GetService("ReplicatedStorage").Remotes.Dumbbell
Event:FireServer()
local Event = game:GetService("ReplicatedStorage").Remotes.Pushup
Event:FireServer()
wait(0.485)
end
end
--Valor
_G.AutoStamina = True
--Fun√ß√£o
function AutoStamina()
while _G.AutoStamina == true do
local Event = game:GetService("ReplicatedStorage").Remotes.Situp
Event:FireServer()
wait(0.485)
end
end
--HubExclusivo
local HubExclusivoTab = Window:MakeTab({
    Name = "MenuExclusivo",
    Icon = "rbxassetid://4483345998",
    PremiumOnly = false
})
local Section = HubExclusivoTab:AddSection({
    Name = "Auto-Farme"
})
HubExclusivoTab:AddToggle({
    Name = "Auto Dumbbell,Pushup,Bench",
    Default = false,
    Callback = function(Value)
        _G.AutoDPB = Value
        AutoDPB()
    end
})
HubExclusivoTab:AddToggle({
    Name = "Farm No Saco",
    Default = false,
    Callback = function(Value)
        _G.AutoGolpe = Value
        AutoGolpe()
    end
})
HubExclusivoTab:AddToggle({
    Name = "Auto Dumbbell,Pushup",
    Default = false,
    Callback = function(Value)
        _G.AutoDP = Value
        AutoDP()
    end
})
HubExclusivoTab:AddToggle({
    Name = "Auto Stamina",
    Default = false,
    Callback = function(Value)
        _G.AutoStamina = Value
        AutoStamina()
    end
})
